                android:textSize="@dimen/textSizeLetter"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.06" >

            <Button
                android:id="@+id/ButtonU"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.20"
                android:background="@drawable/u"
                android:onClick="onClick"
                android:contentDescription="U"
                android:textSize="@dimen/textSizeLetter"
                android:textStyle="bold" />

            <Button
                android:id="@+id/ButtonV"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.20"
                android:background="@drawable/v"
                android:onClick="onClick"
                android:contentDescription="V"
                android:textSize="@dimen/textSizeLetter"
                android:textStyle="bold" />

            <Button
                android:id="@+id/ButtonW"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.20"
                android:background="@drawable/w"
                android:onClick="onClick"
                android:contentDescription="W"
                android:textSize="@dimen/textSizeLetter"
                android:textStyle="bold" />

            <Button
                android:id="@+id/ButtonX"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.20"
                android:background="@drawable/x"
                android:onClick="onClick"
                android:contentDescription="X"
                android:textSize="@dimen/textSizeLetter"
                android:textStyle="bold" />

            <Button
                android:id="@+id/ButtonY"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.20"
                android:background="@drawable/y"
                android:onClick="onClick"
                android:contentDescription="Y"
                android:textSize="@dimen/textSizeLetter"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.06" >

            <Button
                android:id="@+id/buttonZ"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.20"
                android:background="@drawable/z"
                android:onClick="onClick"
                android:contentDescription="Z"
                android:textSize="@dimen/textSizeLetter"
                android:textStyle="bold" />

            <Button
                android:id="@+id/btPTBR"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_margin="3dp"
                android:layout_weight="0.66"
                android:background="@drawable/galinha_deitada"
                android:onClick="onClickSelectLanguage" />

        </LinearLayout>

    </LinearLayout>

    <VideoView
        android:id="@+id/videoView"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignWithParentIfMissing="true"
        android:layout_centerHorizontal="true"
        android:layout_centerInParent="true"
        android:layout_centerVertical="true"
        android:layout_weight="0.06" />

</RelativeLayout>